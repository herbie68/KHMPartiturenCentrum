    <LinearGradientBrush x:Key="ComboBox.Dropdown.Background" StartPoint="0,0" EndPoint="0,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Offset="0.0" Color="#FFF" />
                <GradientStop Offset="1.0" Color="#BCCAF1" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ComboBox.BorderBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Offset="0.0" Color="#BCCAF1" />
                <GradientStop Offset="1.0" Color="#27387A" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ComboBox.DropDown.IsMouseOver" StartPoint="0,0" EndPoint="0,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Offset="0.0" Color="White" />
                <GradientStop Offset="1.0" Color="#707db2" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ComboBox.DropDown.IsPressed" StartPoint="0,0" EndPoint="0,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Offset="0.0" Color="#707db2" />
                <GradientStop Offset="0.1" Color="#BCCAF1" />
                <GradientStop Offset="0.9" Color="#BCCAF1" />
                <GradientStop Offset="1.0" Color="White" />
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>

